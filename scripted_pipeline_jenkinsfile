pipeline {
    agent none
    stages {
        stage('Build') {
            agent {
                docker { image 'httpd' }
            }
            steps {
                sh "pwd"
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
